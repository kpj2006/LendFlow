{
  "name": "defi-lending-protocol",
  "version": "1.0.0",
  "description": "Revolutionary DeFi lending protocol with dynamic orderbook matching that protects small borrowers from whale impact",
  "main": "index.js",
  "keywords": [
    "defi",
    "lending",
    "protocol",
    "orderbook",
    "matching",
    "small-borrower-protection",
    "whale-impact",
    "rootstock",
    "bitcoin",
    "ethereum",
    "smart-contracts",
    "web3",
    "blockchain"
  ],
  "author": "DeFi Lending Protocol Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/lending-protocol.git"
  },
  "homepage": "https://github.com/your-username/lending-protocol#readme",
  "bugs": {
    "url": "https://github.com/your-username/lending-protocol/issues"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "compile": "hardhat compile",
    "compile:watch": "hardhat compile --watch",
    "test": "hardhat test",
    "test:watch": "hardhat test --watch",
    "test:coverage": "hardhat coverage",
    "test:gas": "REPORT_GAS=true hardhat test",
    "deploy": "hardhat run scripts/deploy.js --network rootstock",
    "deploy:local": "hardhat run scripts/deploy.js --network localhost",
    "deploy:testnet": "hardhat run scripts/deploy.js --network rootstockTestnet",
    "deploy:mainnet": "hardhat run scripts/deploy.js --network rootstock",
    "node": "hardhat node",
    "clean": "hardhat clean",
    "size": "hardhat size-contracts",
    "validate-env": "node validate-env.js",
    "setup": "npm install --legacy-peer-deps && npm run compile",
    "demo": "npm run setup && npm run dev",
    "verify": "hardhat verify --network rootstock",
    "flatten": "hardhat flatten",
    "solhint": "solhint 'contracts/**/*.sol'",
    "prettier": "prettier --write .",
    "format": "npm run prettier && npm run lint:fix"
  },
  "dependencies": {
    "@chainlink/contracts": "^1.4.0",
    "@pythnetwork/pyth-sdk-js": "^0.3.0",
    "@pythnetwork/pyth-sdk-solidity": "^4.2.0",
    "@rainbow-me/rainbowkit": "^1.3.0",
    "autoprefixer": "^10.4.0",
    "axios": "^1.12.2",
    "ethers": "^6.8.0",
    "graphql-request": "^7.2.0",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "pino-pretty": "^10.3.1",
    "postcss": "^8.4.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "recharts": "^2.8.0",
    "tailwindcss": "^3.3.0",
    "viem": "^1.19.0",
    "wagmi": "^1.4.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@openzeppelin/contracts": "^4.9.6",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "dotenv": "^17.2.2",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "hardhat": "^2.19.0",
    "typescript": "^5.0.0"
  }
}
